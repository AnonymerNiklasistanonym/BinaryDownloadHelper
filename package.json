{
  "name": "windowsnoninstallerprogramsdownloadhelper",
  "description": "Helps to install programs on Windows that do not have an installer",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "npm-run-all tsc",
    "tsc": "tsc",
    "start": "node .",
    "dev": "cross-env NODE_DEBUG=app* NODE_ENV=development npm-run-all build start",
    "dev:watch": "nodemon --watch src -e ts,json --exec npm run dev",
    "lint": "eslint -c .eslintrc.js --ext .ts src --fix",
    "schemas:build": "tsc --project schemas/tsconfig.json",
    "schemas:start": "node dist/createTsTypeFromJsonSchema.js",
    "schemas": "npm-run-all schemas:build schemas:start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnonymerNiklasistanonym/WindowsNonInstallerProgramsDownloadHelper.git"
  },
  "author": "AnonymerNiklasistanonym",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnonymerNiklasistanonym/WindowsNonInstallerProgramsDownloadHelper/issues"
  },
  "homepage": "https://github.com/AnonymerNiklasistanonym/WindowsNonInstallerProgramsDownloadHelper#readme",
  "dependencies": {
    "got": "^11.2.0",
    "node-7z": "^2.1.1",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/node": "^14.0.6",
    "@types/prettier": "^2.0.1",
    "@types/request": "^2.48.5",
    "@types/unzip": "^0.1.1",
    "@types/unzipper": "^0.10.3",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-plugin-jsdoc": "^26.0.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "json-schema-to-typescript": "^9.1.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.9.3"
  }
}
